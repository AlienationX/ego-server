{
    "dev": {
        "aws_region": "eu-north-1",
        "django_settings": "server.settings",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "exclude_glob": [
            ".git", 
            ".idea", 
            "*.pyc", 
            "__pycache__",
            "package/",
            "wallpaper/scripts/",
            "ego-dev-*.zip"
        ],
        "profile_name": "zappa-profile",
        "project_name": "ego",
        "runtime": "python3.10",
        "s3_bucket": "zappa-tvg8ew9xj",
        "keep_warm": true,
        "keep_warm_expression": "rate(5 minutes)",
        "environment_variables": {
            "DJANGO_SECRET_KEY": "your-secret-key"
        }
    }
}

